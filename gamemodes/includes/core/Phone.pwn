hook OnPlayerText(playerid, text[])
{
	if(MobilePlayer[playerid][calling] == 1)
	{
        new string[158];
        format(string, sizeof(string), "(cellphone) %s noi: %s", GetPlayerNameEx(playerid), text);
        ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
        SendClientMessage(MobilePlayer[playerid][caller], COLOR_YELLOW, string);
        return 0;
    }
    else if(MobilePlayer[playerid][calling] == 3)
    {
        new str[158];
        new str2[158];
        format(str2,sizeof(str2), "%s",text);
        GetPlayerName(playerid, str, 24);
        format(str, sizeof(str), "Tin Nhan moi tu %d (%s): %s", PlayerInfo[playerid][pPnumber], str2);
        SendClientMessage(MobilePlayer[playerid][caller], COLOR_YELLOW, str);
        SendClientMessage(playerid, 0xBFC0C2FF, "Tin Nhan Da Duoc Gui!");
        KillTimer(TimerWaitCalling[playerid]);
        MobilePlayer[playerid][calling] = 0;
        return 0;
	}
    return 1;
}

