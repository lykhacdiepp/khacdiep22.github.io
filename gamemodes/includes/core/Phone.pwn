CMD:s3(playerid, params[])
{
    if(PlayerInfo[playerid][pPnumber])
    {
        if(!GetPVarInt(playerid,"phones3")) GalaxyS3Phone(playerid,1);
        else GalaxyS3Phone(playerid,0);
        return 1;  
    }
    else return SendClientMessage(playerid, 0xFFFF00AA, "Ban khong co iphone!");
}

CMD:answer(playerid, params[])
{
        if(MobilePlayer[playerid][calling] == 2)
        {              
            SetPlayerAttachedObject(playerid, 9, 330, 6);
            SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
            KillTimer(TimerWaitCalling[MobilePlayer[playerid][caller]]);
            MobilePlayer[playerid][calling] = 1;
            MobilePlayer[MobilePlayer[playerid][caller]][calling] = 1;
            SendClientMessage(playerid, 0xFFFF00AA, "Cuoc Goi Bat Dau!");
            SendClientMessage(MobilePlayer[playerid][caller], 0xFFFF00AA, "Cuoc Goi Bat Dau!");
        }
        return 1;
}
CMD:hang(playerid, params[])
{
		if(MobilePlayer[playerid][calling] == 1)
		{
		    KillTimer(TimerWaitCalling[MobilePlayer[playerid][caller]]);
		    KillTimer(TimerWaitCalling[playerid]);
	        SendClientMessage(playerid, 0xFFFF00AA, "Ban da ngat cuoc goi!");
	        SendClientMessage(MobilePlayer[playerid][caller], 0xFFFF00AA, "Nguoi ben kia da ngat cuoc goi!");
  			MobilePlayer[MobilePlayer[playerid][caller]][calling] = 0;
            MobilePlayer[MobilePlayer[playerid][caller]][caller] = INVALID_PLAYER_ID;
  			MobilePlayer[playerid][calling] = 0;
            MobilePlayer[playerid][caller] = INVALID_PLAYER_ID;
            RemovePlayerAttachedObject(playerid, 9);
            RemovePlayerAttachedObject(caller, 9);
            SetPlayerSpecialAction(caller, SPECIAL_ACTION_STOPUSECELLPHONE);
            SetPlayerSpecialAction(playerid, SPECIAL_ACTION_STOPUSECELLPHONE);
		}
		else if(MobilePlayer[playerid][calling] == 2)
		{
		    KillTimer(TimerWaitCalling[playerid]);
  			KillTimer(TimerWaitCalling[MobilePlayer[playerid][caller]]);
	        SendClientMessage(playerid, 0xFFFF00AA, "Ban da tu choi cuoc goi!");
	        SendClientMessage(MobilePlayer[playerid][caller], 0xFFFF00AA, "Nguoi ben kia da tu choi cuoc goi...");
  			MobilePlayer[MobilePlayer[playerid][caller]][calling] = 0;
            MobilePlayer[MobilePlayer[playerid][caller]][caller] = INVALID_PLAYER_ID;
  			MobilePlayer[playerid][calling] = 0;
            MobilePlayer[playerid][caller] = INVALID_PLAYER_ID;
            RemovePlayerAttachedObject(playerid, 9);
            RemovePlayerAttachedObject(caller, 9);
            SetPlayerSpecialAction(caller, SPECIAL_ACTION_STOPUSECELLPHONE);
            SetPlayerSpecialAction(playerid, SPECIAL_ACTION_STOPUSECELLPHONE);
		}
		return 1;
}
